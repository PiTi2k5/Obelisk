{
	"title": "Missions",
	"type": "object",
	"properties": {
		"version": {
			"title": "Version",
			"description": "Data version of the format <gamemajor>.<gameminor>.<specificversion>, e.g. '306.83.4740398'",
			"type": "string"
		},
		"format": {
			"title": "Format",
			"description": "Free-form format identifier, changed only when incompatibilities are introduced",
			"type": "string"
		},
		"mod": {
			"title": "Mod info",
			"description": "Source module information",
			"allOf": [
				{
					"$ref": "#/definitions/ModInfo"
				}
			]
		},
		"persistentLevel": {
			"title": "Persistentlevel",
			"type": "string"
		},
		"dispatchers": {
			"title": "Dispatchers",
			"type": "array",
			"items": {
				"$ref": "#/definitions/MissionDispatcher"
			}
		}
	},
	"required": [
		"version",
		"format",
		"persistentLevel",
		"dispatchers"
	],
	"additionalProperties": false,
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"ModInfo": {
			"title": "ModInfo",
			"type": "object",
			"properties": {
				"id": {
					"title": "Mod ID",
					"type": "string"
				},
				"tag": {
					"title": "Blueprint tag",
					"description": "Used in place of the mod ID in blueprint paths",
					"type": "string"
				},
				"title": {
					"title": "Mod name",
					"type": "string"
				}
			},
			"required": [
				"id",
				"tag",
				"title"
			]
		},
		"MissionDispatcher": {
			"title": "MissionDispatcher",
			"type": "object",
			"properties": {
				"type": {
					"title": "Type",
					"description": "Friendly mission type name, or internal ID if unknown.",
					"anyOf": [
						{
							"type": "string"
						},
						{
							"type": "integer"
						}
					]
				},
				"missions": {
					"title": "Missions",
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"x": {
					"title": "X",
					"type": "number"
				},
				"y": {
					"title": "Y",
					"type": "number"
				},
				"z": {
					"title": "Z",
					"type": "number"
				},
				"lat": {
					"title": "Lat",
					"type": "number"
				},
				"long": {
					"title": "Long",
					"type": "number"
				}
			},
			"required": [
				"type",
				"missions",
				"x",
				"y",
				"z"
			],
			"additionalProperties": false
		}
	}
}